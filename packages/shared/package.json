{
  "name": "@misc-poc/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "yarn exec tsc --build --clean && yarn exec tsc --build",
    "build:incremental": "yarn exec tsc --build",
    "test": "jest",
    "lint": "cd ../../.. && yarn exec eslint packages/shared/src --ext .ts",
    "typecheck": "yarn exec tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^24.3.0",
    "@types/uuid": "^9.0.0",
    "jest": "^29.0.0",
    "jest-util": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.9.2"
  }
}
