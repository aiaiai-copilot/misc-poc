{
  "name": "@misc-poc/backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "test -d src && yarn exec tsc --build",
    "test": "jest",
    "lint": "test -d src && yarn exec eslint src --ext .ts",
    "typecheck": "yarn exec tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "dev": "node --loader ts-node/esm src/index.ts",
    "start": "node dist/index.js",
    "migration:generate": "yarn exec typeorm migration:generate -d src/infrastructure/database/typeorm.config.ts",
    "migration:create": "yarn exec typeorm migration:create -d src/infrastructure/database/typeorm.config.ts",
    "migration:run": "yarn exec typeorm migration:run -d src/infrastructure/database/typeorm.config.ts",
    "migration:revert": "yarn exec typeorm migration:revert -d src/infrastructure/database/typeorm.config.ts",
    "migration:show": "yarn exec typeorm migration:show -d src/infrastructure/database/typeorm.config.ts",
    "schema:drop": "yarn exec typeorm schema:drop -d src/infrastructure/database/typeorm.config.ts",
    "db:create-db": "node --loader ts-node/esm src/infrastructure/database/cli.ts create-db",
    "db:status": "node --loader ts-node/esm src/infrastructure/database/cli.ts status",
    "db:run": "node --loader ts-node/esm src/infrastructure/database/cli.ts run",
    "db:revert": "node --loader ts-node/esm src/infrastructure/database/cli.ts revert",
    "db:validate": "node --loader ts-node/esm src/infrastructure/database/cli.ts validate",
    "db:validate-checksums": "node --loader ts-node/esm src/infrastructure/database/cli.ts validate-checksums",
    "db:export-history": "node --loader ts-node/esm src/infrastructure/database/cli.ts export-history",
    "db:validate-history": "node --loader ts-node/esm src/infrastructure/database/cli.ts validate-history"
  },
  "dependencies": {
    "@misc-poc/application": "workspace:*",
    "@misc-poc/domain": "workspace:*",
    "@misc-poc/infrastructure-cache": "workspace:*",
    "@types/redis": "^4.0.11",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "stream-json": "^1.9.1",
    "typeorm": "^0.3.27",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.6.0",
    "@testcontainers/redis": "^11.7.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^6.0.2",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.11.10",
    "@types/stream-chain": "^2.1.0",
    "@types/stream-json": "^1.7.8",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.0.0",
    "jest-util": "^29.0.0",
    "js-yaml": "^4.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.9.2"
  }
}
